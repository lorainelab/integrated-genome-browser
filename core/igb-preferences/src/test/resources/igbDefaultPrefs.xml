<prefs>

    <!-- Specify servers here with type, name, and url.  Type is "das", "das2", or "quickload" -->

    <!-- servers added by default -->
    <server type="quickload" name="IGB Quickload" url="http://igbquickload.org/quickload/" order="0" default="true" enabled="true" mirror="http://bioviz.org/quickload/"/>
    <server type="quickload" name="Pollen" url="http://www.igbquickload.org/pollen" order="1" default="true" enabled="true" mirror="http://bioviz.org/pollen/"/>
    <server type="quickload" name="Cytokinin" url="http://www.igbquickload.org/cyto" order="2" default="true" enabled="true" mirror="http://bioviz.org/cytopub/"/>
    <server type="quickload" name="Blueberry" url="http://www.igbquickload.org/blueberry" order="3" default="true" enabled="true" mirror="http://bioviz.org/berrypub/"/>
    <server type="das" name="UCSC" url="http://genome.cse.ucsc.edu/cgi-bin/das/dsn" order="5" default="true" enabled="true"/>
    <server type="das" name="Ensembl" url="http://www.ensembl.org/das/dsn" enabled="false" order="6" default="true"/>
    <server type="quickload" name="BioVizServerCache" url="http://bioviz.org/cached/" enabled="true" primary="true" order="7" default="true"/>

    <!-- caching of slow servers (e.g., UCSC and Ensembl) -->


    <!-- servers disabled by default -->


    <!-- Specify plugin repositories here with name, and url -->

    <repository name="Bioviz" url="http://bioviz.org/igbserver" enabled="true" default="true"/>

    <repository name="Bioviz development" url="http://eos.transvar.org/igb_plugins" enabled="false" default="true"/>

    <!-- <annotation_url> associates a track title or "id" regular expression with a URL.
           parameters:
             annot_type_regex: feature name and a regular expression (case insensitive by default)
             annot_id_regex: annotation id and a regular expression (case insensitive by default)
             url:               a url.  Every instance of "$$" (if any) will be
                                substituted with the item ID when forming links.
    -->
    <!-- use (?i) at the start of a regex to force case-insensitive matching of the whole regex -->
    <!-- always support Google searching -->
    <annotation_url annot_id_regex=".*"
                    name="Google"
                    url="http://www.google.com/search?q=$$"
                    type="default"
                    image_icon_path="16x16/actions/google.png"
    />


    <!-- Any RefSeq mRNA alignment -->
    <annotation_url annot_type_regex="(EST)|(mRNA)|(RefGene)|(RefSeq)" 
                    name="NCBI"
                    url="http://www.ncbi.nlm.nih.gov/nuccore/$$"
                    type="default"
                    image_icon_path="16x16/actions/ncbi.png"
    />


    <!-- model organism databases -->
    <!-- Arabidopsis thaliana -->
    <!-- Match AGI gene models names. -->
    <annotation_url annot_id_regex="^AT[1-5CM]G\d{5,}\.\d+$"
                    name="TAIR"
                    url="http://www.arabidopsis.org/servlets/TairObject?type=gene&amp;name=$$"
                    type="default"
                    image_icon_path="16x16/actions/tair.png"
    />


    <!-- slime mold -->
    <annotation_url annot_id_regex="^DDB_G\d+$"
                    name="DictyBase Gene"
                    url="http://dictybase.org/gene/$$"
                    type="default"
    />
    <annotation_url annot_id_regex="^DDB\d+$"
                    name="DictyBase"
                    url="http://www.dictybase.org/db/cgi-bin/search/search.pl?query=$$"
                    type="default"
    />

    <!-- fission yeast -->
    <annotation_url annot_type_regex="(?i).*sanger_pombe$"
                    name="GeneDB"
                    url="http://www.genedb.org/genedb/Search?name=$$&amp;organism=pombe&amp;desc=yes&amp;wildcard=yes&amp;searchId=Search"
                    type="default"
    />


    <!-- corn -->
    <!-- annotation titles should be something like "Maize Transcripts 5b release" -->
    <annotation_url annot_type_regex=".*Maize Transcripts.*"
                    name="MaizeSequence.org"
                    url="http://www.maizesequence.org/Zea_mays/Transcript/Transcript?t=$$"
                    type="default"
    />

    <!-- budding yeast -->
    <annotation_url annot_type_regex="S. cerevisiae genes"
                    name="SGD"
                    url="http://www.yeastgenome.org/cgi-bin/locus.fpl?locus=$$" 
                    type="default"
    />

    <!-- Affymetrix Link-outs -->
    <annotation_url annot_type_regex="(?i).* netaffx consensus"
                    name="NetAffx"
                    url="https://www.affymetrix.com/analysis/netaffx/fullrecord.affx?pk=$$"
                    type="default"
    />
    <annotation_url annot_type_regex="(?i).*Ex-1_0-st.*Probes$"
                    name="NetAffx Probeset Details"
                    url="http://www.affymetrix.com/analysis/netaffx/exon/probe_set.affx?pk=$$"
                    type="default"
    />
    <annotation_url annot_type_regex="(?i).*Ex-1_0-st.*Transcripts$"
                    name="NetAffx Transcript Details"
                    url="http://www.affymetrix.com/analysis/netaffx/exon/transcript.affx?pk=$$"
                    type="default"
    />
    <!-- db_xref linkouts -->
    <!-- see http://www.ncbi.nlm.nih.gov/projects/collab/db_xref.html to support more --> 
    <annotation_url annot_id_regex="^FLYBASE:.*$" id_field="db_xref" 
                    name="Flybase"
                    url="http://flybase.org/.bin/fbidq.html?$$"
                    type="default"
    />
    <annotation_url annot_id_regex="^dictyBase:.*$" id_field="db_xref" 
                    name="DictyBase"
                    url="http://dictybase.org/db/cgi-bin/search/search.pl?query=$$"
                    type="default"
    />
    <annotation_url annot_id_regex="^GeneID:.*$" id_field="db_xref" 
                    name="Entrez"
                    url="http://www.ncbi.nlm.nih.gov/gene/$$"
                    type="default"
    />



    <annotation_url annot_id_regex=".*NCBI.*"
                    name="Entrez cross-database search"
                    url="http://www.ncbi.nlm.nih.gov/gquery?term=$$"
                    type="default"
    />
  
</prefs>
