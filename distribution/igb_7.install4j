                            <string>boolean IS_WINDOWS = System.getProperty("os.name").toLowerCase().contains("windows");
                                                            String igbHomeDirName = ".igb";
                                                            String igbDataHome = "";
                                                            if (IS_WINDOWS) {
                                                            igbDataHome = System.getenv("AppData") + File.separator + "IGB";
                                                            } else {
                                                            igbDataHome = System.getProperty("user.home") + File.separator + igbHomeDirName;
                                                            }
                                                            File igbDataHomeFile = new File(igbDataHome);
                                                            if (!igbDataHomeFile.mkdir()) {
                                                            File osgiBundleCacheDir = new File(igbDataHome + File.separator + "bundles");
                                                            osgiBundleCacheDir.mkdir();
                                                            context.setVariable("osgiBundleCacheDir", igbDataHome + File.separator + "bundles");
                                                            }else{
                                                            context.setVariable("osgiBundleCacheDir", "");
                                                            }


